#include <stdio.h>

int main()
{
    int n1;
    int n2;
    int d;
    
    printf("Infome o primeiro valor:");
    scanf("%d",&n1);
    
    printf("Infome o segundo valor:");
    scanf("%d",&n2);
    
    while(n2 == 0){
         printf("Infome o segundo valor:");
    scanf("%d",&n2);
        
    }
    
    d= n1/n2;
    
    printf("o resultado da divisão é %d",d);

    return 0;
}
